<%- search_field_list(params).each do |field_def| -%>
<div class="advsearch_field<%= ' range_field' if field_def[:range]  %>">
  <% if field_def[:range] %>
    <h4><%= field_def[:display_label] %></h4>
    <label for="<%=field_def[:key]%>_start">Start</label>
    <%= text_field_tag "#{field_def[:key]}_start", range_value(field_def[:key])[:start] %>
		<label for="<%=field_def[:key]%>_end">End</label>
    <%= text_field_tag "#{field_def[:key]}_end", range_value(field_def[:key])[:end] %>
  <% else %>
    <h4><%= label_tag field_def[:key], "#{field_def[:display_label] }" %></h4>
    <%= text_field_tag field_def[:key], label_tag_default_for(field_def[:key]) %>
  <% end %>
  </div>
<%- end -%>

