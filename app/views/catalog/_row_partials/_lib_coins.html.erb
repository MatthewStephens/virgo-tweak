<tr class="document" id="<%= document[:id] %>">
  <td><!--number --></td>
	<td class="folder_container"><%= link_to 'Remove Star', folder_path(document[:id]), :class=>'deleteFolder_link remove-star', :title=>'Remove Star' %></td>
	<td class="title">
		<% document.title_type_fields.each do |k,v| %><%= link_to_document document, :label => :title_display %><% end %>
	</td>
	<td class="author"><span class="starred-author"></td>
	<td class="format"><%= document.value_for :format_facet %></td>
	<td class="year"></td>
	<td class="type">Catalog</td>
	<td class="access"><%= link_to 'View Complete Record', document.value_for(:url_display) %></td>
</tr>
